{
  "suggest.echodocSupport": true,
  "suggest.noselect": false,
  "suggest.minTriggerInputLength": 0,
  "suggest.enablePreview": true,
  "suggest.maxCompleteItemCount": 200,
  "java.errors.incompleteClasspath.severity": "ignore",
  "suggest.snippetIndicator": " ►",
  "yank.highlight.duration": 350,
  "python.jediEnabled": false,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "coc.preferences.jumpCommand": "drop",
  "coc.source.around.firstMatch": false,
  "coc.source.buffer.firstMatch": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.customIcons": {
    "icons": {
      "markdown": {
        "code": "",
        "color": "#7fa5d4"
      }
    }
  },
  "explorer.keyMappings.global": {
    "o": ["expandable?", "expandOrCollapse", "open"],
    "<esc>": "quit"
  },
  "latex.lint.onSave": false,
  "diagnostic.warningSign": "",
  "list.insertMappings": {
    "<C-j>": "action:open",
    "<C-r>": "do:refresh",
    "<C-f>": "do:right",
    "<C-b>": "do:left",
    "<A-j>": "normal:j",
    "<A-k>": "normal:k",
    "<C-t>": "action:tabe",
    "<C-x>": "call:MyFunc",
    "<A-v>": "eval:@@"
  },
  "list.source.bibtex.files": [""]
}
