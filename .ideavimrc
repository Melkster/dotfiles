set surround
set multiple-cursors
set showmode
set scrolloff=5
set relativenumber
set smartcase

let mapleader = "\<Space>"
map <leader>y "+y
map <leader>Y "+Y
map <leader>d "+d
map <leader>D "+D
map <leader>p "+p
map <leader>P "+P
nmap <C-j> o<Esc>
map! <M-j> <Down>
map! <M-k> <Up>
map <C-w><C-v> <C-w>v
map <C-w><C-c> <C-w>c
map <C-w><C-c> <C-w>c
